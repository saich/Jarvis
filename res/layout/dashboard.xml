<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >

    <com.google.android.apps.iosched.ui.widget.DashboardLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">>
        
        <Button android:id="@+id/dashboard_button_calls"
            style="@style/DashboardButton"
            android:text="@string/label_calls"
            android:drawableTop="@drawable/dashboard_button_calls" />
        
         <Button android:id="@+id/dashboard_button_music"
            style="@style/DashboardButton"
            android:text="@string/label_music"
            android:drawableTop="@drawable/dashboard_button_music" />
         
    </com.google.android.apps.iosched.ui.widget.DashboardLayout>
    
</LinearLayout>